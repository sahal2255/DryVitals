         $(document).ready(function() {
            const rowsPerPage = 5;
            let currentPage = 1;

            // Function to show rows for the current page
            function showPage(page) {
                $('#userTable tbody tr').hide();
                $('#userTable tbody tr').slice((page - 1) * rowsPerPage, page * rowsPerPage).show();
            }

            // Initial page load
            showPage(currentPage);

            // Function to update pagination buttons
            function updatePaginationButtons() {
                $('#prevBtn').prop('disabled', currentPage === 1);
                $('#nextBtn').prop('disabled', currentPage === Math.ceil($('#userTable tbody tr').length / rowsPerPage));
            }

            // Pagination button click events
            $('#prevBtn').click(function() {
                if (currentPage > 1) {
                    currentPage--;
                    showPage(currentPage);
                    updatePaginationButtons();
                }
            });

            $('#nextBtn').click(function() {
                if (currentPage < Math.ceil($('#userTable tbody tr').length / rowsPerPage)) {
                    currentPage++;
                    showPage(currentPage);
                    updatePaginationButtons();
                }
            });
        });
    </script>

