    async function  singleOrders(){
        const orderId = event.target.closest('tr').dataset.orderId
        console.log('single order',orderId)
        try{
            const response =await axios.get(`/admin/singleOrder/${orderId}`)
            if(response){

            window.location.href = `/admin/singleView?id=${orderId}`;
            }

        }
        catch(error){
            console.log('error for single order getting ',error)
        }
    }
</script>
