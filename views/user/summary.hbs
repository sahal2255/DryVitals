</head>

<body>

	<!--PreLoader-->

	<!--PreLoader Ends-->

	<!-- header -->
	<div class="top-header-area" id="sticker">
		<div class="container">
			<div class="row">
				<div class="col-lg-12 col-sm-12 text-center">
					<div class="main-menu-wrap">
						<!-- logo -->
						<div class="site-logo">

							<img src="/assets/img/Mainlogo.png" alt="">
							</a>
						</div>
						<!-- logo -->

						<!-- menu start -->
						<nav class="main-menu">
							<ul>
								
								<li><a href="/">Home</a></li>
								<li><a href="/product">Product</a></li>
								
								
								<li>
									<div class="header-icons">
										{{!-- <a class="mobile-hide search-bar-icon" href="#" id="search-icon"><i class="fas fa-search"></i></a> --}}
										<a class="shopping-cart" href="/cart"><i class="fas fa-shopping-cart"></i></a>
										<a class="mobile-hide wishlist-bar-icon" href="/wishlist"><i class="bi bi-heart-fill"></i></a>
										<a class="mobile-hide person-bar-icon" href="/profile"><i class="bi bi-person-circle"></i></a>
									</div>
									
								</li>
								
							</ul>
						</nav>
						<!-- menu end -->
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end header -->

	<!-- search area -->
	<div class="search-area">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<span class="close-btn"><i class="fas fa-window-close"></i></span>
					<div class="search-bar">
						<div class="search-bar-tablecell">
							<h3>Search For:</h3>
							<input type="text" placeholder="Keywords">
							<button type="submit">Search <i class="fas fa-search"></i></button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end search arewa -->

	<!-- breadcrumb-section -->
	<div class="breadcrumb-section breadcrumb-bg">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 offset-lg-2 text-center">
					<div class="breadcrumb-text">
						<p>Dry and Organic</p>
						<h1>Order Summary</h1>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- end breadcrumb section -->

	<div class="container mt-5 mb-5">
		<div class="row">
		<div class="col-md-12 mb-3">
            <a href="/product" class="btn " style="background-color:rgb(203, 128, 16) ;">Back to Shop</a>
        </div>
			<div class="col-md-12">
				<div class="card shadow h-100" style="background-color: #d9edd7;">
					<div class="card-body">
						<h5 class="card-title text-success mb-4">Product Details</h5>
						{{#each order}}

						<div class="d-flex align-items-center mb-4">
							<img src="{{productDetails.imageUrl}}" alt="Product Image" class="img-fluid mr-3"
								style="width: 100px;">
							<div>
								<h6 class="mb-1">{{productDetails.productName}}</h6>
								<p class="text-muted mb-1"><strong>Product Price:</strong>  ₹ {{cart.product.productPrice}}
								</p>
								<p class="text-muted mb-0"><strong>Variant:</strong> {{cart.product.productVariant}}</p>

								<p class="text-muted mb-0"><strong>Quantity:</strong> {{cart.product.quantity}}</p>
							</div>
						</div>

						{{/each}}
						<div>

							<h6>Total Amount:  ₹ {{currentOrder.totalAmount}}</h6>

						</div>

					</div>
				</div>
			</div>

		</div>
	</div>

<footer class="footer" style="background-color: #343a40; color: #ffffff; padding: 50px 0;">
    <div class="container-lg"> <!-- Changed to container-lg for larger size -->
        <div class="row">
            <div class="col-md-4">
                <h5 style="color: white;">About Us</h5>
                <p style="color: rgb(174, 164, 164);">At Dry Vitals, we are passionate about providing you with the finest selection of dry fruits, dates, and nuts sourced from the most trusted suppliers worldwide. With a commitment to quality, freshness, and customer satisfaction, we aim to bring nature's goodness to your doorstep.</p>
            </div>
            <div class="col-md-4">
                <h5 style="color: white;">Customer Service</h5>
                <ul class="list-unstyled" style="margin-left: 0;">
                    <li><a href="/profile" style="color: #ffffff;">Profile</a></li>
                    <li><a href="/product" style="color: #ffffff;">Product</a></li>
                    <li><a href="/cart" style="color: #ffffff;">Cart</a></li>
                    <li><a href="/wishlist" style="color: #ffffff;">Wishlist</a></li>
                    <li><a href="/order" style="color: #ffffff;">Orders</a></li>
                </ul>
            </div>
            <div class="col-md-4">
                <h5 style="color: white;">Connect with Us</h5>
                <ul class="list-unstyled social-links" style="margin-left: 0;">
                    <li><a href="#" style="color: #ffffff;"><i class="fab fa-facebook-f"></i> Facebook</a></li>
                    <li><a href="#" style="color: #ffffff;"><i class="fab fa-twitter"></i> Twitter</a></li>
                    <li><a href="#" style="color: #ffffff;"><i class="fab fa-instagram"></i> Instagram</a></li>
                    <li><a href="#" style="color: #ffffff;"><i class="fab fa-linkedin-in"></i> LinkedIn</a></li>
                </ul>
            </div>
        </div>
        <hr style="background-color: #ffffff;">
        <div class="row">
            <div class="col-md-6">
                <h5>Payment Methods</h5>
                <ul class="list-inline payment-icons" style="margin-left: 0;">
                    <li class="list-inline-item"><i class="fab fa-cc-visa fa-2x" style="color: #ffffff;"></i></li>
                    <li class="list-inline-item"><i class="fab fa-cc-mastercard fa-2x" style="color: #ffffff;"></i></li>
                    <li class="list-inline-item"><i class="fab fa-cc-paypal fa-2x" style="color: #ffffff;"></i></li>
                    <li class="list-inline-item"><i class="fab fa-cc-amazon-pay fa-2x" style="color: #ffffff;"></i></li>
                </ul>
            </div>
        </div>
    </div>
</footer>


<script>
    document.addEventListener("DOMContentLoaded", function() {
        setTimeout(function() {
            window.location.href = '/product'; // Replace '/product' with the URL of your product page
        }, 30000); // 30 seconds = 30,000 milliseconds
    });
</script>






